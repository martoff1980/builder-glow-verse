{
  "version": 2,
  "builds": [
    { "src": "dist/server/node-build.mjs", "use": "@vercel/node" },
    { "src": "dist/spa/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/spa/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/dist/spa/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server/node-build.mjs"
    }
  ]
}
